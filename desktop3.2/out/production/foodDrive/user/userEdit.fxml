        <JFXTextField id="phone" fx:id="number" layoutX="480.0" layoutY="346.0" promptText="Phone Number" stylesheets="@/admin/adminStyle.css" />
        <JFXPasswordField id="passw" fx:id="pass" layoutX="480.0" layoutY="396.0" promptText="Password" stylesheets="@/admin/adminStyle.css" />
        <JFXPasswordField id="confpassw" fx:id="passconf" layoutX="666.0" layoutY="396.0" promptText="Confirm Password" stylesheets="@/admin/adminStyle.css" />
