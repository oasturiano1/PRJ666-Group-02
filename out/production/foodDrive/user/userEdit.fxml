<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.UserEdit">
    <children>
        <JFXTextField id="fullname" fx:id="fname" layoutX="133.0" layoutY="110.0" promptText="First Name" stylesheets="@/admin/adminStyle.css" />
        <JFXTextField id="fullname" fx:id="lname" layoutX="319.0" layoutY="110.0" promptText="Last Name" stylesheets="@/admin/adminStyle.css" />
        <JFXTextField id="mail" fx:id="email" layoutX="133.0" layoutY="157.0" promptText="Email Address" stylesheets="@/admin/adminStyle.css" />
        <JFXTextField id="phone" fx:id="number" layoutX="133.0" layoutY="206.0" promptText="Phone Number" stylesheets="@/admin/adminStyle.css" />
        <JFXPasswordField id="passw" fx:id="pass" layoutX="133.0" layoutY="256.0" promptText="Password" stylesheets="@/admin/adminStyle.css" />
        <JFXPasswordField id="confpassw" fx:id="passconf" layoutX="319.0" layoutY="256.0" promptText="Confirm Password" stylesheets="@/admin/adminStyle.css" />
        <Label layoutX="243.0" layoutY="44.0" prefHeight="58.0" prefWidth="114.0" text="Edit Profile">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <JFXTextField id="contactname" fx:id="emname" layoutX="133.0" layoutY="309.0" promptText="Contact Name" stylesheets="@/admin/adminStyle.css" />
        <JFXTextField id="contactnumber" fx:id="emnumber" layoutX="317.0" layoutY="309.0" promptText="Contact Phone Number" stylesheets="@/admin/adminStyle.css" />
        <Button id="signup" fx:id="signupbtn" layoutX="360.0" layoutY="352.0" mnemonicParsing="false" onAction="#updateMethod" text="Update Profile" />
        <Label fx:id="error" layoutX="489.0" layoutY="106.0" prefHeight="58.0" prefWidth="89.0" />
      <Button layoutX="133.0" layoutY="352.0" mnemonicParsing="false" onAction="#reset" text="Reset Default" />
      <Button fx:id="cancel" layoutX="21.0" layoutY="19.0" mnemonicParsing="false" onAction="#close" text="Cancel" />
    </children>
</AnchorPane>
