<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="signup.SignUp">
    <children>
        <TextField id="fullname" fx:id="fname" layoutX="218.0" layoutY="72.0" promptText="First and last Name" />
        <TextField id="mail" fx:id="email" layoutX="218.0" layoutY="105.0" promptText="Email Address" />
        <TextField id="phone" fx:id="number" layoutX="218.0" layoutY="138.0" promptText="Phone Number" />
        <PasswordField id="passw" fx:id="pass" layoutX="219.0" layoutY="175.0" promptText="Password" />
        <PasswordField id="confpassw" fx:id="passconf" layoutX="219.0" layoutY="206.0" promptText="Confirm Password" />
        <Label layoutX="252.0" layoutY="14.0" prefHeight="58.0" prefWidth="81.0" text="Register">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Button id="signup" fx:id="signupbtn" layoutX="264.0" layoutY="352.0" mnemonicParsing="false" onAction="#signUpMethod" text="Sign Up" />
        <TextField id="contactname" fx:id="emname" layoutX="218.0" layoutY="266.0" promptText="Name" />
        <TextField id="contactnumber" fx:id="emnumber" layoutX="218.0" layoutY="302.0" promptText="Phone Number" />
        <Label layoutX="242.0" layoutY="240.0" text="Emergency Contact" />
        <Label fx:id="error" layoutX="220.0" layoutY="335.0" prefHeight="17.0" prefWidth="193.0" />
    </children>
</AnchorPane>
